{% extends 'base.html.twig' %}

{% block body %}
    <h1>Obrafinalizacion</h1>

    <table>
        <tbody>
            <tr>
                <th>Fecha</th>
                <td>{% if obraFinalizacion.fecha %}{{ obraFinalizacion.fecha|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ obraFinalizacion.id }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('obrafinalizacion_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('obrafinalizacion_edit', { 'id': obraFinalizacion.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
