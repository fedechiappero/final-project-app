{% extends 'base.html.twig' %}

{% block body %}
    <h1>Preciositiotarea</h1>

    <table>
        <tbody>
            <tr>
                <th>Material</th>
                <td>{{ precioSitioTarea.material }}</td>
            </tr>
            <tr>
                <th>Manoobra</th>
                <td>{{ precioSitioTarea.manoObra }}</td>
            </tr>
            <tr>
                <th>Ultimaactualizacion</th>
                <td>{% if precioSitioTarea.ultimaActualizacion %}{{ precioSitioTarea.ultimaActualizacion|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ precioSitioTarea.id }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('preciositiotarea_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('preciositiotarea_edit', { 'id': precioSitioTarea.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
