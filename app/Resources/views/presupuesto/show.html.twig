{% extends 'base.html.twig' %}

{% block body %}
    <h1>Presupuesto</h1>

    <table>
        <tbody>
            <tr>
                <th>Fechaemision</th>
                <td>{% if presupuesto.fechaEmision %}{{ presupuesto.fechaEmision|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Valor</th>
                <td>{{ presupuesto.valor }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ presupuesto.id }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('presupuesto_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('presupuesto_edit', { 'id': presupuesto.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
