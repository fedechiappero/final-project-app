{% extends 'base.html.twig' %}

{% block body %}
    <h1>Contratistas</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ contratista.id }}</td>
            </tr>
            <tr>
                <th>Razonsocial</th>
                <td>{{ contratista.razonSocial }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('contratista_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('contratista_edit', { 'id': contratista.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
