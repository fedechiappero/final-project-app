<div id="modalordencompra" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Buscar orden de compra</h4>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    <label>Desde:</label>
                    <input type="date" id="fechadesde">
                </div>
                <button type="button" class="btn" onclick="buscarOrdenes()">Buscar</button>
                <table id="tablaordenes" class="table table-striped">
                    <thead>
                        <tr>
                            <th>Id</th>
                            <th>Numero</th>
                            <th>Emision</th>
                            <th>Proveedor</th>
                            <th>Obra</th>
                        </tr>
                    </thead>
                    <tbody>

                    </tbody>
                </table>
            </div>

            <div class="modal-footer">
                <div class="form-group">
                    <button type="button" class="btn btn-default" data-dismiss="modal" id="close">Cerrar</button>
                </div>
            </div>
        </div>
    </div>
</div>