{% extends 'base.html.twig' %}

{% block body %}
    <h1>Chequepropio</h1>

    <table>
        <tbody>
            <tr>
                <th>Numero</th>
                <td>{{ chequePropio.numero }}</td>
            </tr>
            <tr>
                <th>Fechapago</th>
                <td>{% if chequePropio.fechaPago %}{{ chequePropio.fechaPago|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Fechaemision</th>
                <td>{% if chequePropio.fechaEmision %}{{ chequePropio.fechaEmision|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Importe</th>
                <td>{{ chequePropio.importe }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ chequePropio.id }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('chequepropio_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('chequepropio_edit', { 'id': chequePropio.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
